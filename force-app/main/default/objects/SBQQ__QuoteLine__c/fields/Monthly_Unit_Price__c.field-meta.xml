<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Monthly_Unit_Price__c</fullName>
    <externalId>false</externalId>
    <formula>IF(TEXT(SBQQ__SubscriptionType__c)!= &quot;One-time&quot; &amp;&amp; Disable_Proration__c == false, SBQQ__RegularPrice__c /(SBQQ__DefaultSubscriptionTerm__c*SBQQ__ProrateMultiplier__c), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Monthly Unit Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>4</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
