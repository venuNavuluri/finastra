<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Price_Loan_Grow__c</fullName>
    <externalId>false</externalId>
    <formula>IF(AND(New_Price_Per_Loan__c  &gt; 0, Previous_Price_Per_Loan__c &gt; 0),

(New_Price_Per_Loan__c -  Previous_Price_Per_Loan__c ) / Previous_Price_Per_Loan__c, 

NULL)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>(New_Price_Per_Loan__c -  Previous_Price_Per_Loan__c ) / Previous_Price_Per_Loan__c</inlineHelpText>
    <label>Price/Loan Grow</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
